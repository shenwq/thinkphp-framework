{
  "name": "ffhome/thinkphp-framework",
  "description": "ThinkPHP framework.",
  "keywords": [
    "ThinkPHP"
  ],
  "homepage": "http://ffhome.top/",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "john.shen",
      "email": "john.shen@foxmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "topthink/framework": "^6.0.0",
    "topthink/think-orm": "^2.0",
    "jianyan74/php-excel": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "files": [],
    "psr-4": {
      "ffhome\\framework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ffhome\\tests\\framework\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
